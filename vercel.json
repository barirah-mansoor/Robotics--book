{
  "version": 2,
  "builds": [
    {
      "src": "website/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/build/assets/$1",
      "check": true
    },
    {
      "src": "/(.+\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))$",
      "dest": "/build/$1",
      "check": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "check": true
    }
  ],
  "framework": "docusaurus"
}