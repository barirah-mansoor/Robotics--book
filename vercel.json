{
  "version": 2,
  "builds": [
    {
      "src": "website/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)$",
      "dest": "/assets/$1"
    },
    {
      "src": "^/img/(.*)$",
      "dest": "/img/$1"
    },
    {
      "src": "^/static/(.*)$",
      "dest": "/static/$1"
    },
    {
      "src": "^/.*\\.\\w+$",
      "dest": "/$0"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "framework": "docusaurus"
}